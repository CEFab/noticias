<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style></style>
</head>
<body>
    <header>
        <h2>Noticias</h2>
    </header>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <ul>
                    <% articulos.forEach(function(articulo){ %>
                        <li>
                            <h3><%= articulo.titulo %></h3>
                            <p><%= articulo.contenido %></p>
                        </li>
                    <% }); %>
                </ul>
            </div>
        </div>
    </div>
    
</body>
</html>